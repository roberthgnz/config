    "^~/types/(.*)$",
    "^~/server/(.*)$",
    "",
    "^~/config/(.*)$",
    "^~/utils/(.*)$",
    "",
    "^~/store/(.*)$",
    "",
    "^~/services/(.*)$",
    "",
    "^~/composables/(.*)$",
    "",
    "^~/components/(.*)$",
    "^~/plugins/(.*)$",
    "",
    "^[./]"
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports"]
}
